{
  "name": "weather-with-duduzi-monorepo",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "pnpm --filter frontend dev",
    "dev:backend": "pnpm --filter backend start:dev",
    "dev:all": "pnpm --parallel --filter \"./apps/*\" dev",
    "build": "pnpm --filter frontend build && pnpm --filter backend build",
    "test": "pnpm --filter frontend test"
  },
  "devDependencies": {
    "typescript": "~5.8.2"
  }
}
